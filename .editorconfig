# top-most EditorConfig file
root = true

# Default style
[*]
# Default UTF-8 charset
charset = utf-8
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
# Indent with 4 spaces
indent_style = space
indent_size = 4
# Trim space at end of line
trim_trailing_whitespace = true
